#include <stdio.h>

int main()
{
    char word[10];
    int i = 0;

    while (scanf("%9s", word) == 1) /*text has to be in the same folder*/
    {
        i = i + 1;
        if (i % 2) /* multi of two or not*/
            fprintf(stdout, "%s\n", word);
        else
            fprintf(stderr, "%s\n", word);
    }

    return 0;
}
